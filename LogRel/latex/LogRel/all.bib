	doi = {10.1145/3622817},
	issue_date = {October 2023},
	journal = {Proc. ACM Program. Lang.},
	keywords = {gradual typing, migratory typing, profiling, rational programmer},
	month = {oct},
	number = {OOPSLA2},
	numpages = {30},
	publisher = {Association for Computing Machinery},
	title = {How Profilers Can Help Navigate Type Migration},
	url = {https://doi.org/10.1145/3622817},
	volume = {7},
	year = {2023},
	bdsk-url-1 = {https://doi.org/10.1145/3622817}}

@article{Greenman:2019aa,
	abstract = {In the context of gradual typing, type soundness guarantees the safety of typed code. When untyped code fails to respect types, a runtime check finds the discrepancy. As for untyped code, type soundness makes no promises; it does not protect untyped code from mistakes in type specifications and unwarranted blame.  To address the asymmetry, this paper adapts complete monitoring from the contract world to gradual typing. Complete monitoring strengthens plain soundness into a guarantee that catches problems with faulty type specifications. Furthermore, a semantics that satisfies complete monitoring can easily pinpoint the conflict between a type specification and a value. For gradual typing systems that fail complete monitoring, the technical framework provides a source-of-truth to assess the quality of blame.},
	address = {New York, NY, USA},
